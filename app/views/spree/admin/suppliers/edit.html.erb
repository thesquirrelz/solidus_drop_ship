<% provide(:title, "Editing: #{@object.name} - Suppliers".html_safe) %>
<% provide(:page_title, "Editing: #{@object.name}".html_safe) %>
<% content_for :page_actions do %><%= button_link_to 'Back to suppliers list', spree.admin_suppliers_path, icon: 'arrow-left' %><% end %>

